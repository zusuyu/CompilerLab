fun @main(): i32 {

%entry:
  @x_1 = alloc i32
  store 1, @x_1
  @y_1 = alloc i32
  store 0, @y_1
  %0 = load @x_1
  @result1 = alloc i32
  br %0, %then1, %else1

%then1:
  %1 = load @y_1
  %2 = ne %1, 0
  store %2, @result1
  jump %end1

%else1:
  store 0, @result1
  jump %end1

%end1:
  %3 = load @result1
  ret %3
}
