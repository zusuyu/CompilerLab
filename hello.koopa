fun @main(): i32 {

%entry:
  @x_1 = alloc i32
  store 1, @x_1
  br 111, %then1, %else1

%then1:
  br 222, %then2, %else2

%then2:
  %0 = load @x_1
  %1 = add %0, 222
  store %1, @x_1
  jump %end2

%else2:
  %2 = load @x_1
  %3 = sub %2, 222
  store %3, @x_1
  jump %end2

%end2:
  jump %end1

%end1:
  %4 = load @x_1
  ret %4
}
